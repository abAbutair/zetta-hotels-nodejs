</script>

<script>
    new WOW().init();
</script>

<script type="text/javascript">
    var carousel1 = $('#forControls02').carousel();
    var carousel2 = $('#forControls03').carousel();
    carousel1.on('slide.bs.carousel', function (event) {
        var to = $(event.relatedTarget).index();
        carousel2.carousel(to);
    });
</script>

